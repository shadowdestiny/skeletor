export declare class NgxSkeletonLoaderModule {
    constructor(parentModule?: NgxSkeletonLoaderModule);
    static forRoot(config?: Partial<NgxSkeletonLoaderConfig>): ModuleWithProviders<NgxSkeletonLoaderModule>;
}
